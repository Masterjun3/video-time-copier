{
    "manifest_version": 2,
    "name": "Video Time Copier",
    "version": "1.0",
    "description": "Copies start/end times of videos.",
    "permissions": [
        "activeTab",
        "clipboardWrite",
        "storage"
    ],
    "browser_action": {
        "default_popup": "gui.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "ico.png"
    }
}